package com.ruby.java.ch03제어문;
/*
 * 출력 형태를 다음과 같이 만들기 실습
 * 실습1:
 * $$$$$
 * $$$$
 * $$$
 * $$
 * $
 * 
 * 실습2: 정삼각형 형태로 출력하기
 * $$$$$
 *  $$$
 *   $
 */
public class 실습_3장_9_forfor문 {
	public static void main(String[] args) {
//		실습1
		/*
		 * String s = "$"; for (int i = 5; i >= 1; i--) { for (int j = i; j >= 1; j--) {
		 * System.out.print(s); } System.out.println(); }
		 */
//		실습2
		String s = "$";
		String b = " ";
		for (int i = 5; i>=1; i--) {
			if (i % 2 == 0) continue;
			for (int j = 0; j <= 2; i++) {
			}
		}
	}
}
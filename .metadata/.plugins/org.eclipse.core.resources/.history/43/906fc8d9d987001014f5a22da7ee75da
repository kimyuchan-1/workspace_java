package com.ruby.java.ch02데이터타입;

public class MinBillCount {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 167000 ;
		int w50000, w10000, w5000, w1000;
		
		w50000 = 167000 / 50000 ;
		w10000 = (167000 % 50000) / 10000 ;
		w5000 = ((167000 % 50000) % 10000) / 5000 ;
		w1000 = (((167000 % 50000) % 10000) % 5000) / 1000 ;
		
		System.out.println("5만원: "+w50000+"장 \n 1만원: "+w10000+"장 \n 5천원: "+w5000+"장 \n 1천원: "+w1000+"장");
	}

}
